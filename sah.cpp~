#include <fstream>
using namespace std;

int n,m;

int main() {
	ifstream f("sah.in");
	ofstream g("sah.out");
	f>>n;
	m=n-1;
	for(int i=1; i<=n-2; ++i) {
		int lc=i,p=(i+1)/2;
		if(lc%2==0) --lc;
		for(int j=1; j<=p; ++j) g<<lc<<' ';
		++lc;
		for(int j=p+1; j<=n; ++j) g<<lc<<' ';
		g<<'\n';
	}
	n;
	for(int i=1; i<3; ++i) {
		for(int j=1; j<=n; ++j) g<<m<<' ';
		g<<'\n';
	}
	return 0;
}