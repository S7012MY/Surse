#include <iostream>
#include <fstream>
#include <algorithm>
#include <cstring>
#define DN 1005
#define x first
#define y second
using namespace std;

typedef pair<int,int> per;//le tin pe dos

int n,m,cnt,p1,aib[DN],ind[DN],c[DN][DN];
double tg[DN];
per p[DN];

inline int lsb(int x) {
	return ((x&(x-1))^x);
}

bool cmp(int a,int b) {
	return tg[a]<tg[b];
}

void up(int p) {
	for(;p<=n;p+=lsb(p)) ++aib[p];
}

int query(int p) {
	int r=0;
	for(int i=p; i>0; i-=lsb(i)) r+=aib[i];
	return r;
}

int main() {
	ifstream f("tri2.in");
	ofstream g("tri2.out");
	f>>n;
	for(int i=1; i<=n; ++i) f>>p[i].x>>p[i].y;
	sort(p+1,p+n+1);
	for(int i=1; i<=n; ++i) {
		memset(aib,0,sizeof(aib));
		for(int j=i+1; j<=n; ++j) {
			ind[j]=j;
			tg[j]=((double)p[j].y-p[i].y)/(p[j].x-p[i].x);
		}
		sort(ind+i,ind+n+1,cmp);
		for(int j=i+1; j<=n; ++j) {
			c[i][ind[j]]=query(ind[j]);
			c[ind[j]][i]=c[i][ind[j]];
			up(ind[j]);
		}
	}
	
	return 0;
}